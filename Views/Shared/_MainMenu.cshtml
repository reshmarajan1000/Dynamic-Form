<li class="treeview">
    <a href="#"><i class="fa fa-dashboard"></i> <span>Dashborad - Saved Forms</span>
    <span class="pull-right-container">
        <i class="fa fa-angle-left pull-right"></i>
        </span>
    </a>
    <ul class="treeview-menu" id="menu">
    </ul>
</li>
</ul>

<script>

    $(function() {

        $.ajax(
        {
            type: "POST",
            url: '@Url.Action("GetFormsList", "Dashboard")',
            error: function (result) {
                alert(result);
            },
                success: function (result) {
             
                var menu = '<li><a href="/Dashboard/FormList/">FormList</a></li>';
                    $.each(result, function (key, value) {
                        menu += '<li><a href="/Dashboard/DisplayForm/' + value.id + '">Form #' + value.id +'</a></li>';
                });
                $("#menu").html(menu);
            }
        });
});
</script>